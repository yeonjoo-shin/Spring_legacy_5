<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.naver.s5.member.MemberDAO">

	<insert id="memberAdd" parameterType="com.naver.s5.member.MemberVO">
		insert into member values(#{id},#{upw},#{uname},#{age},#{email},#{phone})
	</insert>
	
	<select id="memberLogin" parameterType="com.naver.s5.member.MemberVO" resultType="com.naver.s5.member.MemberVO" >
		select * from member where id=#{id} and upw=#{upw}
	</select>


</mapper>